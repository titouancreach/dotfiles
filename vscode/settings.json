{
  "files.trimTrailingWhitespace": true,
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "bounded",
  "vim.replaceWithRegister": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.enableEditorConfigSupport": true,
  "editor.semanticHighlighting.enabled": false,
  "git.enableSmartCommit": true,
  "editor.lineHeight": 27,
  "editor.fontLigatures": true,
  "files.autoSave": "onFocusChange",
  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["-"],
      "commands": ["git.openFile"]
    },
    {
      "before": ["+"],
      "commands": ["git.openChange"]
    },
    {
      "before": ["down"],
      "after": [ "g", "j" ]
    },
    {
      "before": ["up"],
      "after": [ "g", "k" ]
    }
  ],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "git.confirmSync": false,
  "git.autofetch": true,
  "debug.onTaskErrors": "abort",
  "editor.wrappingIndent": "indent",
  "keyboard.touchbar.enabled": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      120
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "editor.minimap.enabled": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.suggestSelection": "first",
  "remote.containers.defaultExtensions": [
    "eamodio.gitlens",
    "ms-dotnettools.csharp",
    "sleistner.vscode-fileutils",
    "esbenp.prettier-vscode",
    "bradlc.vscode-tailwindcss",
    "ms-azuretools.vscode-azurefunctions",
    "GitHub.copilot",
    "medo64.render-crlf",
  ],
  "tailwindCSS.includeLanguages": {
    "aspnetcorerazor": "html"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "editor.cursorBlinking": "solid",
  "editor.lineNumbers": "off",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "dart.debugSdkLibraries": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.startupEditor": "none",
  "terminal.integrated.tabs.enabled": true,
  "editor.rulers": [1000],
  "editor.inlineSuggest.enabled": true,
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.bracketPairColorization.enabled": true,
  "code-eol.newlineCharacter": "â†µ",
  "git.ignoreRebaseWarning": true,
  "files.associations": {
    "appsettings.json": "jsonc",
    "appsettings.Development.json": "jsonc",
    "appsettings.Staging.json": "jsonc",
    "appsettings.Production.json": "jsonc",
    "*.cshtml": "aspnetcorerazor"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "git.rebaseWhenSync": true,
  "editor.fontSize": 11,
  "editor.wordWrapColumn": 150,
  "redhat.telemetry.enabled": false,
  "[markdown]": {
      "files.trimTrailingWhitespace": false
  },
  "window.zoomLevel": 1,
  "extensions.ignoreRecommendations": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "Source Code Pro, Menlo, Monaco, 'Courier New', monospace",
  "csharp.semanticHighlighting.enabled": false,
  "workbench.colorTheme": "Visual Studio 2019 Light",
}
